{
  "name": "yy-menu",
  "version": "1.2.0",
  "description": "A traditional menu system for web apps inspired by Electron",
  "keywords": [
    "menu",
    "system",
    "UI",
    "accelerators",
    "Electron",
    "file menu",
    "web app"
  ],
  "main": "dist/menu.js",
  "scripts": {
    "test": "./node_modules/.bin/budo --dir docs docs/code.js:index.js --live",
    "build": "browserify docs/code.js -o docs/index.js",
    "docs": "./node_modules/.bin/jsdoc -c .jsdoc.json",
    "test-docs": "serve docs/jsdoc",
    "transpile": "./node_modules/.bin/babel src/ --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run transpile && npm run build && npm run docs"
  },
  "author": "David Figatner",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "fork-me-github": "^1.0.3",
    "highlight.js": "^9.12.0",
    "jsdoc": "^3.5.5",
    "yy-jsdoc-template": "^1.3.0"
  },
  "dependencies": {
    "clicked": "^1.3.0"
  }
}
